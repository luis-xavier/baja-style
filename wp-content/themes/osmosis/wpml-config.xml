		<custom-field action="copy">grve_disable_content</custom-field>
		<custom-field action="copy">grve_disable_bottom_bar</custom-field>
		<custom-field action="copy">grve_disable_footer</custom-field>
		<custom-field action="copy">grve_disable_copyright</custom-field>
		<custom-field action="copy">grve_page_feature_element</custom-field>
		<custom-field action="copy">grve_page_feature_size</custom-field>
		<custom-field action="copy">grve_page_feature_height</custom-field>
		<custom-field action="copy">grve_page_feature_header_integration</custom-field>
		<custom-field action="copy">grve_page_feature_header_position</custom-field>
		<custom-field action="copy">grve_page_feature_header_style</custom-field>
		<custom-field action="copy">grve_page_feature_effect</custom-field>
		<custom-field action="copy">grve_page_feature_go_to_section</custom-field>
		<custom-field action="translate">grve_page_slider_items</custom-field>
		<custom-field action="copy">grve_page_slider_settings</custom-field>
		<custom-field action="translate">grve_page_map_items</custom-field>
		<custom-field action="copy">grve_page_map_settings</custom-field>
		<custom-field action="translate">grve_page_image_item</custom-field>
		<custom-field action="translate">grve_page_title_item</custom-field>
		<custom-field action="translate">grve_page_video_item</custom-field>
		<custom-field action="copy">grve_post_layout</custom-field>
		<custom-field action="copy">grve_post_sidebar</custom-field>
		<custom-field action="copy">grve_post_type_gallery_mode</custom-field>
		<custom-field action="copy">grve_post_slider_height</custom-field>
		<custom-field action="copy">grve_post_slider_items</custom-field>
		<custom-field action="translate">grve_post_link_url</custom-field>
		<custom-field action="copy">grve_post_link_new_window</custom-field>
		<custom-field action="copy">grve_post_type_audio_mode</custom-field>
		<custom-field action="translate">grve_post_audio_embed</custom-field>
		<custom-field action="translate">grve_post_audio_mp3</custom-field>
		<custom-field action="translate">grve_post_audio_ogg</custom-field>
		<custom-field action="translate">grve_post_audio_wav</custom-field>
		<custom-field action="translate">grve_post_type_video_mode</custom-field>
		<custom-field action="translate">grve_post_video_embed</custom-field>
		<custom-field action="translate">grve_post_video_mp4</custom-field>
		<custom-field action="translate">grve_post_video_ogv</custom-field>
		<custom-field action="translate">grve_post_video_webm</custom-field>
		<custom-field action="translate">grve_portfolio_description</custom-field>
		<custom-field action="translate">grve_portfolio_details</custom-field>
		<custom-field action="copy">grve_portfolio_layout</custom-field>
		<custom-field action="copy">grve_portfolio_sidebar</custom-field>
		<custom-field action="copy">grve_disable_portfolio_fields_bar</custom-field>
		<custom-field action="copy">grve_disable_portfolio_recent</custom-field>
		<custom-field action="copy">grve_portfolio_media_selection</custom-field>
		<custom-field action="translate">grve_portfolio_video_webm</custom-field>
		<custom-field action="translate">grve_portfolio_video_mp4</custom-field>
		<custom-field action="translate">grve_portfolio_video_ogv</custom-field>
		<custom-field action="translate">grve_portfolio_video_embed</custom-field>
		<custom-field action="copy">grve_portfolio_slider_items</custom-field>
		<custom-field action="copy">grve_portfolio_slider_settings</custom-field>
		<custom-field action="copy">grve_portfolio_link_mode</custom-field>
		<custom-field action="translate">grve_portfolio_link_url</custom-field>
		<custom-field action="copy">grve_portfolio_link_new_window</custom-field>
		<custom-field action="copy">grve_portfolio_link_extra_class</custom-field>
		<custom-field action="translate">grve_testimonial_name</custom-field>
		<custom-field action="translate">grve_testimonial_identity</custom-field>
	</custom-fields>
	<custom-fields-texts>
		<key name="grve_page_title_item">
			<key name="title"/>
			<key name="caption"/>
		</key>
		<key name="grve_page_image_item">
			<key name="title"/>
			<key name="caption"/>
			<key name="button_text"/>
			<key name="button_url"/>
			<key name="button_text2"/>
			<key name="button_url2"/>
		</key>
		<key name="grve_page_video_item">
			<key name="title"/>
			<key name="caption"/>
			<key name="video_webm" />
			<key name="video_mp4"/>
			<key name="video_ogv"/>
			<key name="button_text"/>
			<key name="button_url"/>
			<key name="button_text2"/>
			<key name="button_url2"/>
		</key>
		<key name="grve_page_slider_items">
			<key name="*">
				<key name="title"/>
				<key name="caption"/>
				<key name="button_text"/>
				<key name="button_url"/>
				<key name="button_text2"/>
				<key name="button_url2"/>
			</key>
		</key>
		<key name="grve_page_map_items">
			<key name="*">
				<key name="title"/>
				<key name="info_text"/>
				<key name="button_text"/>
				<key name="button_url"/>
			</key>
		</key>
	</custom-fields-texts>
	<custom-types>
		<custom-type translate="1">portfolio</custom-type>
		<custom-type translate="1">testimonial</custom-type>
	</custom-types>
	<taxonomies>
		<taxonomy translate="1">portfolio_category</taxonomy>
		<taxonomy translate="1">portfolio_fields</taxonomy>
		<taxonomy translate="1">testimonial_category</taxonomy>
	</taxonomies>
	<admin-texts>
		<key name="grve_osmosis_options">
			<key name="page_404_content" />
			<key name="top_bar_left_text" />
			<key name="top_bar_right_text" />
			<key name="safe_button_option_search_text" />
			<key name="safe_button_option_language_text" />
			<key name="safe_button_option_newsletter_text" />
			<key name="safe_button_option_social_text" />
			<key name="menu_responsive_toggle_text" />
			<key name="blog_custom_title" />
			<key name="blog_custom_description" />
			<key name="search_page_not_found_text" />
			<key name="portfolio_backlink" />
			<key name="portfolio_details_text" />
			<key name="portfolio_slug" />
			<key name="footer_copyright_text" />
			<key name="mc4wp_form" />
			<key name="logo_custom_link_url" />
			<key name="privacy_fallback_content" />
			<key name="privacy_consent_bar_content" />
			<key name="privacy_agreement_button_label" />
			<key name="privacy_preferences_button_label" />
			<key name="privacy_preferences_link_url" />
			<key name="privacy_refresh_button_label" />
			<key name="privacy_consent_modal_content" />
		</key>
	</admin-texts>
	<language-switcher-settings>
		<key name="icl_lang_sel_config">
			<key name="font-current-normal">#444444</key>
			<key name="font-current-hover">#000000</key>
			<key name="background-current-normal">#ffffff</key>
			<key name="background-current-hover">#eeeeee</key>
			<key name="font-other-normal">#444444</key>
			<key name="font-other-hover">#000000</key>
			<key name="background-other-normal">#ffffff</key>
			<key name="background-other-hover">#eeeeee</key>
			<key name="border">#cdcdcd</key>
		</key>
		<key name="icl_lang_sel_footer_config">
			<key name="font-current-normal">#444444</key>
			<key name="font-current-hover">#000000</key>
			<key name="background-current-normal">#ffffff</key>
			<key name="background-current-hover">#eeeeee</key>
			<key name="font-other-normal">#444444</key>
			<key name="font-other-hover">#000000</key>
			<key name="background-other-normal">#ffffff</key>
			<key name="background-other-hover">#eeeeee</key>
			<key name="border">#cdcdcd</key>
		</key>
		<key name="icl_language_switcher_sidebar">0</key>
		<key name="icl_widget_title_show">0</key>
		<key name="icl_lang_sel_type">dropdown</key>
		<key name="icl_lso_link_empty">0</key>
		<key name="icl_lso_flags">0</key>
		<key name="icl_lso_native_lang">1</key>
		<key name="icl_lso_display_lang">1</key>
		<key name="icl_lang_sel_footer">0</key>
		<key name="icl_post_availability">0</key>
		<key name="icl_post_availability_position">below</key>
		<key name="icl_post_availability_text">This post is also available in: %s</key>
	</language-switcher-settings>
</wpml-config>

